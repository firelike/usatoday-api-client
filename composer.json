{
    "name": "firelike/usatoday-api-client",
    "description": "PHP client for USA Today bestsellers API",
    "type": "library",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.2",
        "laminas/laminas-mvc-console": "^1.1",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Firelike\\USAToday\\": "src/"
        }
    }
}
